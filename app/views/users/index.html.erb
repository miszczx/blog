<h1>User list </h1>
<ul>
	<% @user.each do |user| %>
	<li> 
		<%= user.name  %> 
		<%= user.age  %> 
		<%= link_to "Edit", edit_user_path(user) %>
		<%= link_to "Delete", user_path(user), method: :delete %>
		<%= link_to "Show", user_path(user) %>
	</li>
	<% end %>
</ul>

<p>
	<%= link_to "Posts", posts_path %>
</p>
<p>
	<%= link_to "Add user", new_user_path %>
</p>




